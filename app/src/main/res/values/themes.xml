<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="Theme.ByteIcon" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="colorPrimary">#eeeeee</item>
        <item name="colorPrimaryVariant">#eeeeee</item>
        <item name="colorOnPrimary">#ffffff</item>
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <item name="android:windowLightStatusBar" tools:targetApi="m">true</item>
    </style>

    <style name="Theme.ByteIcon.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.ByteIcon.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.ByteIcon.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="Aldialog" parent="@style/ThemeOverlay.MaterialComponents.MaterialAlertDialog">
        <item name="colorPrimary">#1d1d1d</item>
        <item name="textAllCaps">false</item>
    </style>
</resources>